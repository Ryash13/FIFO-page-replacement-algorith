#include<stdio.h>i
#include<stdlib.h>

struct fnode
{
	int pno;
}frames[5];

int ref_str[] = {5,1,2,8,9,7,3,4,1,6,8,1,0,5,7};
int pfct=0,i,n,pno,p,flag=0;
int len=15;

int page_found(int pno)
{
	for(i=0;i<n;i++)
	{
		if(frames[i].pno==pno)
		{
			return i;
		}
		return 0;
	}
}

main()
{
	printf("\nEnter number of frames:- ");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		frames[i].pno = -1;
	}
	printf("\nPAGE NUMBER     PAGE FRAMES     PAGE FAULT     PAGE FAULT COUNT\n");
	printf("...................................................................\n");
	for(p=0;p<len;p++)
	{
		pno = ref_str[p];
		flag = 0;
		if(page_found(pno)==0)
		{
			frames[pfct%n].pno = pno;
			pfct++;
			flag = 1;
		}
		printf("\n%5d\t\t",pno);
		for(i=0;i<n;i++)
		{
			printf("%d ",frames[i]);
		}
		if(flag==1)
			printf("\t\tYES");
		else
			printf("\t\tNO");
		
		printf("\t\t%d",pfct);
	}
	printf("\n..............................................\n");
	printf("Total no of page faults are:- %d",pfct);
}
